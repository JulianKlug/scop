{
    "name": "EfficientNet3DModel_test",
    "n_gpu": 1,

    "arch": {
        "type": "EfficientNet3DModel",
        "args": {}
    },
    "data_loader": {
        "type": "GsdOutcomeDataLoader",
        "args":{
            "imaging_dataset_path": "/Users/jk1/temp/joining_test/small_rescaled_data_set.npz",
            "outcome_file_path":  "/Users/jk1/temp/joining_test/joined_anon_outcomes_2015_2016_2017_2018_df.xlsx",
            "channels": [0,1,2,3],
            "outcome": "combined_mRS_90_days",
            "preload_data": true,
            "batch_size": 2,
            "shuffle": true,
            "validation_split": 0.1,
            "num_workers": 2
        }
    },
    "optimizer": {
        "type": "Adam",
        "args":{
            "lr": 0.001,
            "weight_decay": 0,
            "amsgrad": true
        }
    },
    "loss": "cross_entropy_loss",
    "metrics": [
        "accuracy", "top_k_acc"
    ],
    "lr_scheduler": {
        "type": "StepLR",
        "args": {
            "step_size": 50,
            "gamma": 0.1
        }
    },
    "trainer": {
        "epochs": 200,

        "save_dir": "saved/",
        "save_period": 5,
        "verbosity": 2,
        
        "monitor": "min val_loss",
        "early_stop": 10,

        "tensorboard": true
    }
}
